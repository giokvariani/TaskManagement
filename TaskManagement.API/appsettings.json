{
  "ConnectionStrings": {
    "DefaultConnection": "Server=localhost;initial catalog=TaskManagement;Trusted_Connection=True;TrustServerCertificate=True"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
